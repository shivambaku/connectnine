{
  "editor.tabSize": 2,
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "never",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.quiet": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "animejs",
    "Avenir",
    "itox",
    "itoy",
    "Leaderboard",
    "Nuxt",
    "pinia",
    "unclickable",
    "videogame",
    "vueuse",
    "xytoi"
  ]
}
